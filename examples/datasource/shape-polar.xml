<?xml version="1.0" encoding="utf-8"?>
<Map background-color='blue' srs="+init=epsg:3857">

  <Style name="countries">
    <Rule>
      <PolygonSymbolizer fill="green"/>
    </Rule>
  </Style>

  <Style name="grid">
    <Rule>
      <LineSymbolizer/>
    </Rule>
  </Style>
  
  <Layer name="countries" srs="+init=epsg:3857">
    <StyleName>countries</StyleName>
    <Datasource>
      <Parameter name="type">shape</Parameter>
      <Parameter name="file">data/ne_50m_admin_0_countries_lakes.shp</Parameter>
    </Datasource>
  </Layer>

  <Layer name="grid" srs="+init=epsg:3857">
    <StyleName>grid</StyleName>
    <Datasource>
      <Parameter name="type">shape</Parameter>
      <Parameter name="file">data/ne_10m_graticules_30.shp</Parameter>
    </Datasource>
  </Layer>
  
 
</Map>

